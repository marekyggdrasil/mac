    "@mxssfd/typedoc-theme": "^1.1.3",
    "@types/cli-progress": "^3.11.5",
    "@types/jest": "^29.5.8",
    "@types/mime": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
